<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" />

    <title>Xibalba</title>

    <meta name="document-base"
          content="{{ app.request.getScheme() }}://{{ app.request.getHost() }}{{ app.request.baseUrl }}/"/>
    <base href="{{ app.request.getScheme() }}://{{ app.request.getHost() }}{{ app.request.baseUrl }}/version/{{ asset_version }}/"/>

    <link href="css/app.css" rel="stylesheet">
    <script>
        // enable to override webpacks publicPath
        // var webpackPublicPath = '/';
    </script>
</head>
<body>
<div id="app"></div>
<!-- built files will be auto injected -->
{% block javascripts %}
    <script type="text/javascript" src="js/vendors.js"></script>
    <script type="text/javascript" src="js/app.js"></script>
{% endblock %}
</body>
</html>
